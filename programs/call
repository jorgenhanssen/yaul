// A program utilizing a function

set '3' 0
set '5' 1

// We want to square the value in register 0
set 0 100
call SQUARE 

// We want to square the value in register 1
set 1 100
call SQUARE 

// Print the results
out 0
out 1

// End the program
term

// A function that squares the value at the address in register 100
SQUARE:
mul &100 &100 &100
ret

